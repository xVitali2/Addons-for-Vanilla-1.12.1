<Bindings>
	<Binding name="TRINKETMENU_TOGGLE" description="Toggle TrinketMenu" header="TRINKETMENU">
		TrinketMenu.ToggleFrame(TrinketMenuOptions.DisableToggle=="ON" and TrinketMenu_OptFrame or TrinketMenu_MainFrame)
	</Binding>
	<Binding name="TRINKETMENU_TOP" description="Use Top Trinket">
		UseInventoryItem(13)
	</Binding>
	<Binding name="TRINKETMENU_BOTTOM" description="Use Bottom Trinket">
		UseInventoryItem(14)
	</Binding>
</Bindings>